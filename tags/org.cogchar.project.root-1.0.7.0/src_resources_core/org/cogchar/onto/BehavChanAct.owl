    </owl:ObjectProperty>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/hasMedia -->

    <owl:ObjectProperty rdf:about="http://www.cogchar.org/onto/behavChan_2012/hasMedia">
        <rdfs:subPropertyOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/hasA"/>
    </owl:ObjectProperty>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/hasPredicate -->

    <owl:ObjectProperty rdf:about="http://www.cogchar.org/onto/behavChan_2012/hasPredicate">
        <rdfs:subPropertyOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/hasA"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
