     -->

    


    <!-- http://www.cogchar.org/onto/behavChan_2012/hasContentFilePath -->

    <owl:DatatypeProperty rdf:about="http://www.cogchar.org/onto/behavChan_2012/hasContentFilePath">
        <rdfs:subPropertyOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/hasData"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/hasContentXml -->

    <owl:DatatypeProperty rdf:about="http://www.cogchar.org/onto/behavChan_2012/hasContentXml">
        <rdfs:subPropertyOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/hasData"/>
        <rdfs:range rdf:resource="&rdf;XMLLiteral"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/hasData -->

    <owl:DatatypeProperty rdf:about="http://www.cogchar.org/onto/behavChan_2012/hasData"/>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/hasDurationSec -->

    <owl:DatatypeProperty rdf:about="http://www.cogchar.org/onto/behavChan_2012/hasDurationSec">
        <rdfs:subPropertyOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/hasData"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/hasFormalTimestamp -->

    <owl:DatatypeProperty rdf:about="http://www.cogchar.org/onto/behavChan_2012/hasFormalTimestamp">
        <rdfs:subPropertyOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/hasData"/>
        <rdfs:range rdf:resource="&xsd;dateTimeStamp"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/hasJavaTStampMsec -->

    <owl:DatatypeProperty rdf:about="http://www.cogchar.org/onto/behavChan_2012/hasJavaTStampMsec">
        <rdfs:comment>A long number of millisec using the java convention.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/hasData"/>
        <rdfs:range rdf:resource="&xsd;long"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/hasKeyframeCount -->

    <owl:DatatypeProperty rdf:about="http://www.cogchar.org/onto/behavChan_2012/hasKeyframeCount">
        <rdfs:subPropertyOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/hasData"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/hasQueryText -->

    <owl:DatatypeProperty rdf:about="http://www.cogchar.org/onto/behavChan_2012/hasQueryText">
        <rdfs:subPropertyOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/hasData"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/hasUnixTStampSec -->

    <owl:DatatypeProperty rdf:about="http://www.cogchar.org/onto/behavChan_2012/hasUnixTStampSec">
        <rdfs:subPropertyOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/hasData"/>
        <rdfs:range rdf:resource="&xsd;decimal"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.cogchar.org/onto/behavChan_2012/AcaSTMedia -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/AcaSTMedia">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/SpeechTextMedia"/>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/Agent -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/Agent">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/SysThingy"/>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/AnimFrameMedia -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/AnimFrameMedia">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/FrameMedia"/>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/AnimJobChan -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/AnimJobChan">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/JobChan"/>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/AnimPlayJob -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/AnimPlayJob">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/MediaPlayJob"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.cogchar.org/onto/behavChan_2012/hasChan"/>
                <owl:someValuesFrom rdf:resource="http://www.cogchar.org/onto/behavChan_2012/AnimJobChan"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.cogchar.org/onto/behavChan_2012/hasMedia"/>
                <owl:someValuesFrom rdf:resource="http://www.cogchar.org/onto/behavChan_2012/AnimFrameMedia"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/AudioFrameMedia -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/AudioFrameMedia">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/FrameMedia"/>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/AudioInChan -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/AudioInChan">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/SenseChan"/>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/AudioJobChan -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/AudioJobChan">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/JobChan"/>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/AudioPlayJob -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/AudioPlayJob">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/MediaPlayJob"/>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/BeatInChan -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/BeatInChan">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/PercepChan"/>
        <rdfs:comment>Tells us the signature, tempo, phase, volume, confidence of a discernible rhythm source.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/Behav -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/Behav">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/ModelThingy"/>
        <rdfs:comment>Behav instances are pieces of behavior specification given in terms of 
time,  
channels, 
things (affected by actions), 
and jobs (updated by exec layer).</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/BodySenseChan -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/BodySenseChan">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/SenseChan"/>
        <rdfs:comment>Raw / fused information about our own body state, including servo positions, temperatures, errors, etc.

Some overlap with physic chan.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/BoogieChan -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/BoogieChan">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/GoalChan"/>
        <rdfs:comment>Rhythmically oriented movement channel, i.e. dancing, involving one or more groove things.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/Chan -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/Chan">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/ModelThingy"/>
        <rdfs:comment>A chan represents a boundary between cognition and concrete I/O.  
It may refer to 

 1) a physical(-ish) media channel or network port
 2) a logical channel for assertions in repos
 3) a subtle combo of #1 and #2.

In practice, each piece of information is marked with the URI of the channel that it is VERB-ed on (where verb = sent, received, played, perceived...). </rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/ChanAct -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/ChanAct">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/ChanFrag"/>
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/ThingAct"/>
        <rdfs:comment>Represents a ThingAct strongly associated to a channel (but usually not a JobChannel).  It is normally a control command or an  event.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/ChanFrag -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/ChanFrag">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/ModelThingy"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.cogchar.org/onto/behavChan_2012/hasChan"/>
                <owl:someValuesFrom rdf:resource="http://www.cogchar.org/onto/behavChan_2012/Chan"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A fragment is a &quot;thing&quot; which is simply any piece of data referring to a channel.   

Arbitrary perceptions, goals, and thoughts may be captured as fragments.    

To &quot;send a message&quot; of the most general kind, convey a ThingAct which CREATEs a Fragment on some channel.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/ChanFrame -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/ChanFrame">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/ChanFrag"/>
        <rdfs:comment>Describes a particular data-frame associated to some channel, which is part of a continuous stream of data on that channel (rather than a special &quot;command&quot; or &quot;event&quot;).

The content data of the frame may be encoded in RDF somewhere, or it may not be.  </rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/ChanJob -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/ChanJob">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/Job"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.cogchar.org/onto/behavChan_2012/hasChan"/>
                <owl:someValuesFrom rdf:resource="http://www.cogchar.org/onto/behavChan_2012/JobChan"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/ChoiceInChan -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/ChoiceInChan">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/SymbolChan"/>
        <rdfs:comment>A user&apos;s explicit choice in some structured scenario is indicated on a ChoiceInChan.

Menu selections, restricted vocabulary interactions, and multiple-choice games all make use of this kind of chanel.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/Command -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/Command">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/SysThingy"/>
        <rdfs:comment>Command is a direct instruction from client software or some GUI that is typically more disruptive than a channel message.   

Commands cannot usually  be received from NUI, it must use channels.    

Commands may be sent using ThingActs, but they may also be sent directly in code.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/CommandInChan -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/CommandInChan">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/SymbolChan"/>
        <rdfs:comment>Explicit commands may be conveyed on a Command chan, but they may also be conveyed through direct coding.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/FrameMedia -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/FrameMedia">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/Media"/>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/GazeChan -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/GazeChan">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/GoalChan"/>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/GestureChan -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/GestureChan">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/Chan"/>
        <rdfs:comment>Some expressive or indicative gesture, usually involving physical movement, with various parameters and constraints.  Some overlap with various GoalChans, but a Gesture can sometimes be relatively simple, e.g. open loop, but based on body starting state.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/GoalChan -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/GoalChan">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/Chan"/>
        <rdfs:comment>Represents macro control over closed-loop physical behaviors like gazing, pointing, walking, running, dancing.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/Goody -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/Goody">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/SysThingy"/>
        <rdfs:comment>A goody is generally just some arbitrary useful shape in a virtual world.  
It is included in this ontology because it was the first working example
of an entity controlled by ThingActs.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/HububInChan -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/HububInChan">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/PercepChan"/>
        <rdfs:comment>Hubub vector refers to directions of interest from audio + video fusion.  Includes camera saliency (e.g. motion) and audio location.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/Implication -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/Implication">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/SysThingy"/>
        <rdfs:comment>An implication defines a consequence asserted when a particular Predicate becomes satisfied.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/Job -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/Job">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/ModelThingy"/>
        <rdfs:comment>A Job represents a temporarily continuous stream-oriented action that may be monitored and controlled.   

Typically a Job involves the playing of Media.

A Job is controlled and monitored using ThingActs.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/JobChan -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/JobChan">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/Chan"/>
        <rdfs:comment>JobChan is a marker for an output Chan that has or may have jobs associated to it.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/LocomotionChan -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/LocomotionChan">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/GoalChan"/>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/LoqSTMedia -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/LoqSTMedia">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/SpeechTextMedia"/>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/Media -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/Media">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/#Thingy"/>
        <rdfs:comment>Media provides metadata and optionally some subset of actual content.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/MediaPlayJob -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/MediaPlayJob">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/ChanJob"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.cogchar.org/onto/behavChan_2012/hasMedia"/>
                <owl:someValuesFrom rdf:resource="http://www.cogchar.org/onto/behavChan_2012/Media"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/ModelThingy -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/ModelThingy">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/#Thingy"/>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/OnceBehav -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/OnceBehav">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/Behav"/>
        <rdfs:comment>A OnceBehav may fire its action only once unless &amp; until it is explicitly reset.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/PercepChan -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/PercepChan">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/Chan"/>
        <rdfs:comment>A perceived entity input channel.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.cogchar.org/onto/behavChan_2012/PersonInChan -->

    <owl:Class rdf:about="http://www.cogchar.org/onto/behavChan_2012/PersonInChan">
        <rdfs:subClassOf rdf:resource="http://www.cogchar.org/onto/behavChan_2012/PercepChan"/>
        <rdfs:comment>Fused/fragmentary information about people we perceive, based on faces, skeletons, voices.  </rdfs:comment>
