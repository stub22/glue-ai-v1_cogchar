	bc:jointNum			42;			bc:jointName			"Root_Spin";
	bc:defaultPosNorm	0.5;
	
	bc:projectionRange	[
		# This resource is a "blank node", because it doesn't have an explicit URI.
		bc:boneName			"Root";
		bc:rotationAxis		"X_ROT";
		bc:minAngleDeg		-180.0;
		bc:maxAngleDeg		 180.0;
