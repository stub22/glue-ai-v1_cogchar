###  http://onto.robokind.info/onto/201407/exInstLpath_OWL2#lesson_X88

:lesson_X88 rdf:type lpath:Lesson ,
                     owl:NamedIndividual ;
            
            rdfs:label "A label for the a pretend lesson where facil chooses some content." ;
            
            rdfs:comment "Milo wants to teach you stuff." ;
            
            lpath:hasWrittenName "Lesson X88 (play me!)" ;
            
            lpath:hasChildBlock :presChar_LX88C55 ,
                                :presVid_LX88V77 ,
                                :quest_LX88Q01_whatContentDoyaWant ;
            
            lpath:hasChildConstraint :tc_LX88_endsWith_EitherPayloadContentEnded ,
                                     :tc_LX88_startsWith_Q01 ,
                                     :tc_storyBtn_trigs_charStory ,
                                     :tc_vidButton_trigs_vidShow .



###  http://onto.robokind.info/onto/201407/exInstLpath_OWL2#lesson_X99

:lesson_X99 rdf:type lpath:Lesson ,
                     owl:NamedIndividual ;
            
            lpath:hasWrittenName "Lesson X99 (empty!)" ;
            
            rdfs:comment "This lesson is full of good info.  Come and learn from Milo!" .



###  http://onto.robokind.info/onto/201407/exInstLpath_OWL2#module_X

:module_X rdf:type lpath:Module ,
                   owl:NamedIndividual ;
          
          lpath:hasWrittenName "Module X" ;
          
          lpath:hasChildLesson :lesson_X88 ,
                               :lesson_X99 .



###  http://onto.robokind.info/onto/201407/exInstLpath_OWL2#presChar_LX88C55

:presChar_LX88C55 rdf:type lpath:CharacterPresentation ,
                           owl:NamedIndividual ;
                  
                  lpath:hasSpokenText "Yes, I am a Giants fan.  Hunter Pence is fast and fierce.  Vamos Gigantes!" ;
                  
                  rdfs:comment """Pretend char presentation points to an anim and also hasSpeechText.
It nominally presentsToRole : Facilitator, so the robot may  look at the
facilitator if possible, but this property is optional for Character content.""" ;
                  
                  lpath:presentsToRole lpath:DFLT_FACILITATOR ;
                  
                  lpath:hasChildAnimPointer :ptrAnim_XPA_44 .



###  http://onto.robokind.info/onto/201407/exInstLpath_OWL2#presVid_LX88V77

:presVid_LX88V77 rdf:type lpath:VideoPresentation ,
                          owl:NamedIndividual ;
                 
                 rdfs:comment """Pretend video presentation - this instance is part of the main content, so is namestamped appropriately.
The video pointer we refer to is usually from a separate MediaPointer volume.
presentsToRole asserts that both Facilitator and Student should see the video, if possible, although that is not a common practice...""" ;
                 
                 lpath:presentsToRole lpath:DFLT_FACILITATOR ,
                                      lpath:DFLT_STUDENT ;
                 
                 lpath:hasChildVideoPointer :ptrVid_XPV_66 .



###  http://onto.robokind.info/onto/201407/exInstLpath_OWL2#ptrAnim_XPA_44

:ptrAnim_XPA_44 rdf:type lpath:AnimMediaPointer ,
                         owl:NamedIndividual ;
                
                lpath:hasPathText "/path/to/anim/hunterPenceIsSafe.xml" .



###  http://onto.robokind.info/onto/201407/exInstLpath_OWL2#ptrImage_iconForMovie

:ptrImage_iconForMovie rdf:type lpath:ImageMediaPointer ,
                                owl:NamedIndividual ;
                       
                       lpath:hasPathText "path/to/movieIcon.png" .



###  http://onto.robokind.info/onto/201407/exInstLpath_OWL2#ptrImage_iconForRobot

:ptrImage_iconForRobot rdf:type lpath:ImageMediaPointer ,
                                owl:NamedIndividual ;
                       
                       lpath:hasPathText "path/to/robotIcon.png" .



###  http://onto.robokind.info/onto/201407/exInstLpath_OWL2#ptrVid_XPV_66

:ptrVid_XPV_66 rdf:type lpath:VideoMediaPointer ,
                        owl:NamedIndividual ;
               
               lpath:hasPathText "/path/to/neat/video.mp4" ;
               
               rdfs:comment "Note that the sample URI is not related to the lesson content that uses this pointer." .



###  http://onto.robokind.info/onto/201407/exInstLpath_OWL2#qTimeout_LX88Q01

:qTimeout_LX88Q01 rdf:type lpath:QuestionTimeoutPrompt ,
                           owl:NamedIndividual ;
                  
                  lpath:hasMaxDurationMsec "8025"^^xsd:int .



###  http://onto.robokind.info/onto/201407/exInstLpath_OWL2#qstAnsBtn_LX88Q01A01_showMeTheVideo

:qstAnsBtn_LX88Q01A01_showMeTheVideo rdf:type lpath:QuestionPromptGuiButton ,
                                              owl:NamedIndividual ;
                                     
                                     lpath:hasScreenText "Please show the video" ;
                                     
                                     lpath:hasStyleText "some CSS style for the video button" ;
                                     
                                     lpath:hasChildImagePointer :ptrImage_iconForMovie ;
                                     
                                     lpath:followsPromptButton :qstAnsBtn_LX88Q01A02_tellMeTheStory .



###  http://onto.robokind.info/onto/201407/exInstLpath_OWL2#qstAnsBtn_LX88Q01A02_tellMeTheStory

:qstAnsBtn_LX88Q01A02_tellMeTheStory rdf:type lpath:QuestionPromptGuiButton ,
                                              owl:NamedIndividual ;
                                     
                                     lpath:hasScreenText "Screen Text for Button Y" ;
                                     
                                     lpath:hasStyleText "some CSS style for the robot-story button" ;
                                     
                                     lpath:hasChildImagePointer :ptrImage_iconForRobot .



###  http://onto.robokind.info/onto/201407/exInstLpath_OWL2#quest_LX88Q01_whatContentDoyaWant

:quest_LX88Q01_whatContentDoyaWant rdf:type lpath:SingleGuiQuestion ,
                                            owl:NamedIndividual ;
                                   
                                   lpath:hasScreenText "Which content would you like to see?" ;
                                   
                                   lpath:presentsToRole lpath:DFLT_FACILITATOR ,
                                                        lpath:DFLT_STUDENT ;
                                   
                                   lpath:hasChildPrompt :qTimeout_LX88Q01 ,
                                                        :qstAnsBtn_LX88Q01A01_showMeTheVideo ,
                                                        :qstAnsBtn_LX88Q01A02_tellMeTheStory .



###  http://onto.robokind.info/onto/201407/exInstLpath_OWL2#tc_LX88_endsWith_EitherPayloadContentEnded

:tc_LX88_endsWith_EitherPayloadContentEnded rdf:type lpath:AwaitsAny ,
