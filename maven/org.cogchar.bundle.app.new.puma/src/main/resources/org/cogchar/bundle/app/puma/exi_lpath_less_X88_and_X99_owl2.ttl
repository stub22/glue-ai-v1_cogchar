@prefix : <http://onto.robokind.info/onto/201407/exInstLpath_OWL2#> .
@prefix exi: <http://onto.robokind.info/onto/201407/exInstLpath_OWL2#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix lpath: <http://onto.robokind.info/onto/201407/LessonPath_OWL2#> .
@base <http://onto.robokind.info/onto/201407/exInstLpath_OWL2> .

<http://onto.robokind.info/onto/201407/exInstLpath_OWL2> rdf:type owl:Ontology ;
                                                         
                                                         rdfs:comment "Contains some example instance data for the LessonPath domain." ;
                                                         
                                                         owl:imports <http://onto.robokind.info/onto/201407/LessonPath_OWL2> .


#################################################################
#                                                               #
#    Setup Module                                               # 
#                                                               #
#################################################################

###  http://onto.robokind.info/onto/201407/exInstLpath_OWL2#module_X

:module_X rdf:type lpath:Module ,
                   owl:NamedIndividual ;
          
          lpath:hasWrittenName "Games" ;
          lpath:hasWrittenBriefName "Games" ;
          lpath:hasWrittenDescription "Games" ;
          lpath:hasChildMember :lessonModuleMember_X99 .

###  http://onto.robokind.info/onto/201407/exInstLpath_OWL2#lessonModuleMember_X99

:lessonModuleMember_X99 rdf:type lpath:LessonModuleMember ,
                                 owl:NamedIndividual ;
                        
                        lpath:hasWrittenDescription "Bens test of making Red Light Green Light with loops." ;
                        lpath:hasWrittenBriefName "Red Light Green Light Dancing Game" ;
                        lpath:hasWrittenName "Red Light Green Light" ;
                        lpath:hasChildLesson :lesson_LOOP_SIMPLE_QUESTION .


#################################################################
#                                                               #
#    Setup Lesson                                               # 
#                                                               #
#################################################################

:qstAnsBtn_LOOP_SIMPLE_QUESTION_B2_S03_QuestionLoop_STUDENT_Q_ANS_2
        a                           lpath:ContentBlock , lpath:QuestionPromptGuiButton , lpath:Prompt ;
        lpath:followsPromptButton   :qstAnsBtn_LOOP_SIMPLE_QUESTION_B2_S03_QuestionLoop_STUDENT_Q_ANS_1;
        lpath:hasChildImagePointer  :ptrBtnImg_LOOP_SIMPLE_QUESTION_B2_S03_QuestionLoop_STUDENT_Q_ANS_2;
        lpath:hasScreenText         "End Lesson" ;
        lpath:hasStyleText          "red" .

:blkSeq_LOOP_SIMPLE_QUESTION_B4_Outro
        a                         lpath:LoopingBlock , lpath:ContentBlock ;
        lpath:hasChildBlock       :presChar_LOOP_SIMPLE_QUESTION_B4_S03_Outro_END;
        lpath:hasChildConstraint  :tc_LOOP_SIMPLE_QUESTION_B4_S03_Outro_END_finishLesson,
                                  :tc_LOOP_SIMPLE_QUESTION_B4_S03_Outro_END_startBlock .

:tc_LOOP_SIMPLE_QUESTION_B2_S03_QuestionLoop_STUDENT_startBlock
        a                         lpath:TimingConstraint , lpath:AwaitsAny ;
        lpath:startsBlock         :ques_LOOP_SIMPLE_QUESTION_B2_S03_QuestionLoop_STUDENT;
        lpath:waitsForBlockStart  :loopB_LOOP_SIMPLE_QUESTION_B2_QuestionLoop.

:tc_LOOP_SIMPLE_QUESTION_B1_S03_Intro_INTRO_startBlock
        a                         lpath:TimingConstraint , lpath:AwaitsAny ;
        lpath:startsBlock         :presChar_LOOP_SIMPLE_QUESTION_B1_S03_Intro_INTRO;
        lpath:waitsForBlockStart  :blkSeq_LOOP_SIMPLE_QUESTION_B1_Intro.

:presChar_LOOP_SIMPLE_QUESTION_B4_S03_Outro_END
        a                          lpath:ContentBlock , lpath:CharacterPresentation ;
        lpath:hasChildAnimPointer  :ptrAnim_LOOP_SIMPLE_QUESTION_B4_S03_Outro_END_01;
        lpath:hasSpokenText        "This demo is now finished. \\mrk=500\\ " .

:qstAnsBtn_LOOP_SIMPLE_QUESTION_B2_S03_QuestionLoop_STUDENT_Q_ANS_1
        a                           lpath:ContentBlock , lpath:PromptButton , lpath:QuestionPromptGuiButton , lpath:Prompt ;
        lpath:hasChildImagePointer  :ptrBtnImg_LOOP_SIMPLE_QUESTION_B2_S03_QuestionLoop_STUDENT_Q_ANS_1;
        lpath:hasScreenText         "Restart Loop" ;
        lpath:hasStyleText          "green" .

:tc_LOOP_SIMPLE_QUESTION_B4_S03_Outro_END_finishLesson
        a                          lpath:TimingConstraint , lpath:AwaitsAny ;
        lpath:finishesBlock        :loopB_LOOP_SIMPLE_QUESTION_B2_QuestionLoop,
                                   :lesson_LOOP_SIMPLE_QUESTION ;
        lpath:waitsForBlockFinish  :presChar_LOOP_SIMPLE_QUESTION_B4_S03_Outro_END.

:ques_LOOP_SIMPLE_QUESTION_B2_S03_QuestionLoop_STUDENT
        a                     lpath:ContentBlock , lpath:SingleGuiQuestion ;
        lpath:hasChildPrompt  :qstAnsBtn_LOOP_SIMPLE_QUESTION_B2_S03_QuestionLoop_STUDENT_Q_ANS_2,
                              :qstAnsBtn_LOOP_SIMPLE_QUESTION_B2_S03_QuestionLoop_STUDENT_Q_ANS_1 ;
        lpath:hasScreenText   "Restart Loop?" ;
        lpath:presentsToRole  lpath:DFLT_STUDENT .

:presChar_LOOP_SIMPLE_QUESTION_B1_S03_Intro_INTRO
        a                          lpath:ContentBlock , lpath:CharacterPresentation ;
        lpath:hasChildAnimPointer  :ptrAnim_LOOP_SIMPLE_QUESTION_B1_S03_Intro_INTRO_01;
        lpath:hasSpokenText        " \\rspd=85\\ This is a simple looping demo that utilizes a question. \\mrk=0\\ " .

:lesson_LOOP_SIMPLE_QUESTION
        a                         lpath:Lesson , lpath:ContentBlock ;
        lpath:hasChildBlock       :blkSeq_LOOP_SIMPLE_QUESTION_B4_Outro,
                                  :loopB_LOOP_SIMPLE_QUESTION_B2_QuestionLoop ,
                                  :blkSeq_LOOP_SIMPLE_QUESTION_B1_Intro ;
        lpath:hasChildConstraint  :tc_LOOP_SIMPLE_QUESTION_B2_QuestionLoop_external_startSequence,
                                  :tc_LOOP_SIMPLE_QUESTION_B1_Intro_external_startSequence ;
        lpath:hasWrittenName      "LOOP_SIMPLE_QUESTION" .

:tc_LOOP_SIMPLE_QUESTION_B4_S03_Outro_END_startBlock
        a                         lpath:TimingConstraint , lpath:AwaitsAny ;
        lpath:startsBlock         :presChar_LOOP_SIMPLE_QUESTION_B4_S03_Outro_END;
        lpath:waitsForBlockStart  :blkSeq_LOOP_SIMPLE_QUESTION_B4_Outro.

:blkSeq_LOOP_SIMPLE_QUESTION_B3_RestartQuestion
        a                    lpath:LoopingBlock , lpath:ContentBlock ;
        lpath:hasChildBlock  :presChar_LOOP_SIMPLE_QUESTION_B3_S03_RestartQuestion_ALREADY_DANCING.

:loopB_LOOP_SIMPLE_QUESTION_B2_QuestionLoop
        a                         lpath:LoopingBlock , lpath:ContentBlock ;
        lpath:hasChildBlock       :blkSeq_LOOP_SIMPLE_QUESTION_B3_RestartQuestion,
                                  :ques_LOOP_SIMPLE_QUESTION_B2_S03_QuestionLoop_STUDENT ;
        lpath:hasChildConstraint  :tc_LOOP_SIMPLE_QUESTION_B4_Outro_internal_startSequence,
                                  :tc_LOOP_SIMPLE_QUESTION_B3_S04_RestartQuestion_RESTART_LOOP_startBlock ,
                                  :tc_LOOP_SIMPLE_QUESTION_B3_S03_RestartQuestion_ALREADY_DANCING_startBlock ,
                                  :tc_LOOP_SIMPLE_QUESTION_B3_RestartQuestion_internal_startSequence ,
                                  :tc_LOOP_SIMPLE_QUESTION_B2_S03_QuestionLoop_STUDENT_startBlock .

:presChar_LOOP_SIMPLE_QUESTION_B3_S03_RestartQuestion_ALREADY_DANCING
        a                    lpath:ContentBlock , lpath:CharacterPresentation ;
        lpath:hasSpokenText  "The loop will now restart." .

:tc_LOOP_SIMPLE_QUESTION_B3_RestartQuestion_internal_startSequence
        a                          lpath:TimingConstraint , lpath:AwaitsAny ;
        lpath:startsBlock          :blkSeq_LOOP_SIMPLE_QUESTION_B3_RestartQuestion;
        lpath:waitsForBlockFinish  :qstAnsBtn_LOOP_SIMPLE_QUESTION_B2_S03_QuestionLoop_STUDENT_Q_ANS_1.

:tc_LOOP_SIMPLE_QUESTION_B1_Intro_external_startSequence
        a                         lpath:TimingConstraint , lpath:AwaitsAny ;
        lpath:startsBlock         :blkSeq_LOOP_SIMPLE_QUESTION_B1_Intro;
        lpath:waitsForBlockStart  :lesson_LOOP_SIMPLE_QUESTION.

:tc_LOOP_SIMPLE_QUESTION_B2_QuestionLoop_external_startSequence
        a                          lpath:TimingConstraint , lpath:AwaitsAny ;
        lpath:restartsLoop         :loopB_LOOP_SIMPLE_QUESTION_B2_QuestionLoop;
        lpath:startsBlock          :loopB_LOOP_SIMPLE_QUESTION_B2_QuestionLoop;
        lpath:waitsForBlockFinish  :presChar_LOOP_SIMPLE_QUESTION_B1_S03_Intro_INTRO.

:blkSeq_LOOP_SIMPLE_QUESTION_B1_Intro
        a                         lpath:LoopingBlock , lpath:ContentBlock ;
        lpath:hasChildBlock       :presChar_LOOP_SIMPLE_QUESTION_B1_S03_Intro_INTRO;
        lpath:hasChildConstraint  :tc_LOOP_SIMPLE_QUESTION_B1_S03_Intro_INTRO_startBlock.

:tc_LOOP_SIMPLE_QUESTION_B3_S04_RestartQuestion_RESTART_LOOP_startBlock
        a                          lpath:TimingConstraint , lpath:AwaitsAny ;
        lpath:restartsLoop         :loopB_LOOP_SIMPLE_QUESTION_B2_QuestionLoop;
        lpath:startsBlock          :loopB_LOOP_SIMPLE_QUESTION_B2_QuestionLoop;
        lpath:waitsForBlockFinish  :presChar_LOOP_SIMPLE_QUESTION_B3_S03_RestartQuestion_ALREADY_DANCING.

lpath:DFLT_STUDENT  a  lpath:Role .

:tc_LOOP_SIMPLE_QUESTION_B4_Outro_internal_startSequence
        a                          lpath:TimingConstraint , lpath:AwaitsAny ;
        lpath:startsBlock          :blkSeq_LOOP_SIMPLE_QUESTION_B4_Outro;
        lpath:waitsForBlockFinish  :qstAnsBtn_LOOP_SIMPLE_QUESTION_B2_S03_QuestionLoop_STUDENT_Q_ANS_2.

:tc_LOOP_SIMPLE_QUESTION_B3_S03_RestartQuestion_ALREADY_DANCING_startBlock
        a                         lpath:TimingConstraint , lpath:AwaitsAny ;
        lpath:startsBlock         :presChar_LOOP_SIMPLE_QUESTION_B3_S03_RestartQuestion_ALREADY_DANCING;
        lpath:waitsForBlockStart  :blkSeq_LOOP_SIMPLE_QUESTION_B3_RestartQuestion.
